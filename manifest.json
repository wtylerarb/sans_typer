{
    "manifest_version": 2,
    "name": "Sans Typer",
    "version": "1.0",
    "description": "Plays a sound when typing.",
    "permissions": [
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "sounds/sans_voice.mp3"
    ]
}